{
  "env": {
    "es6": true,
    "node": true,
    "amd": true,
    "jest": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint"],
  "ignorePatterns": ["/dist/**"],
  "rules": {
    "camelcase": [2, { "properties": "never" }],
    "no-console": "off"
  }
}
